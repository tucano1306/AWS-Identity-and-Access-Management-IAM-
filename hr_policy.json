{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket",
          "s3:GetObject",
          "dynamodb:Query",
          "dynamodb:Scan"
        ],
        "Resource": [
          "arn:aws:s3:::example-bucket/*",
          "arn:aws:dynamodb:us-west-2:123456789012:table/EmployeeData"
        ]
      }
    ]
  }